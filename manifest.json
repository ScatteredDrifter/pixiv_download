{
    "browser_specific_settings": {
      "gecko": {
        "strict_min_version": "58.0a1"
      }
    },
    "background":{
      "scripts":["testapp.js"]
    },
    "icons": {
        "48": "icon.png"
      },
    "browser_action": {
      "browser_style": true,
      "default_title": "helppls",
      "default_popup": "testpopup.html"
    },
    "description": "Download Images from pixiv, more easily ... or something like that",
    "homepage_url": "https://github.com/ScatteredDrifter",
    "manifest_version": 2,
    "name": "helppls",
    "permissions": [
      "tabs",
      "downloads",
      "activeTab",
      "<all_urls>"
    ],
    "version": "1.0"
  }
  